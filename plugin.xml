<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plguin-live" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0">
    <name>CDVLive</name>
    <js-module name="CDVLive" src="www/CDVLive.js">
        <clobbers target="cordova.plugins.live" />
    </js-module>

    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="CDVLive">
                <param name="ios-package" value="CDVLive" />
            </feature>
        </config-file>

        <header-file src="src/ios/CDVLive.h" />
        <source-file src="src/ios/CDVLive.m" />
    </platform>
</plugin>