<?xml version='1.0' encoding='utf-8'?>
<plugin id="chat1v1.chatcall" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>ChatCall</name>
    <js-module name="LiveCall" src="www/LiveCall.js">
        <clobbers target="cordova.plugins.live" />
    </js-module>

    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="ChatCall">
                <param name="ios-package" value="ChatCall" />
            </feature>
        </config-file>

        <header-file src="src/ios/CDVLive.h" />
        <source-file src="src/ios/CDVLive.m" />
    </platform>
</plugin>